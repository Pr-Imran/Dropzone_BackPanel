        function uploadPhoto() {
            var imgfile = document.getElementById('imagePath');
            var formData = new FormData();
            formData.append('employeeID', $("#employeeID").val());
            formData.append('photographID', $("#photographID").val());
            formData.append('empPhoto', $('input[type=file]')[0].files[0]);
            console.log(formData)
            console.log($("#employeeID").val())
            console.log($("#photographID").val())
            swal({
                title: 'Are you sure?',
                text: 'Do you want to update this photo!',
                type: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, Update it!'
            }).then(function () {
                $.ajax({
                    url: '@Url.Action("ProfilePictureSave", "Photograph", new { area = "HRPMSEmployee" })',
                    data: formData,
                    type: 'POST',
                    dataType: 'json',
                    contentType: false,
                    processData: false,
                })
                    .done(function (response) {
                        console.log(response);
                        if (response == "success") {
                            swal('', 'Update Successfully!', 'success').
                                then(function () {
                                    location.reload();
                                })
                        }
                        else {
                            swal('', 'Unable to Update!', 'warning');
                        }
                    })
                    .fail(function () {
                        swal('', 'Unable to Update!', 'warning');
                    });
            });
        }



        function getpasswordvalidation() {

            var pass = $("#Password").val();

            Common.Ajax('GET', '/api/PassSecurityPolicy/CheckPasswordPolicyFields/' + pass, [], 'json', ajaxFunctionCheckPasswordPolicyFields, false);

        }
        function ajaxFunctionCheckPasswordPolicyFields(response) {

            console.log("ajaxFunctionCheckPasswordPolicyFields");
            console.log('response', response)
            console.log(response.length)
            var option = "";
            if (parseInt(response.length) > 0) {
                $.each(response, function (i, item) {
                    console.log(item);
                    option += `
                            <p style="color:red;">${item}</p>
                            `
                    $('#passId').empty();
                    $('#passId').append(option);
                })

                localStorage.setItem('isValid', false);
            }
            else {
                $('#passId').empty();
                localStorage.setItem('isValid', true);

            }

        }



